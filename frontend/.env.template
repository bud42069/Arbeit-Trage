# Frontend Environment Configuration Template
# Copy this file to .env and adjust for your environment

# Backend API URL
# For local development: http://localhost:8001
# For production: https://your-domain.com
REACT_APP_BACKEND_URL=http://localhost:8001

# WebSocket port (for development)
WDS_SOCKET_PORT=443

# Disable visual edits in production
REACT_APP_ENABLE_VISUAL_EDITS=false

# Disable health check polling
ENABLE_HEALTH_CHECK=false
